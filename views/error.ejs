<% layout("/layout/boilerplate") %>
<div class="alert alert-danger mt-2" role="alert">
    <h1><%= statusCode %></h1>
    <h4 class="alert-heading">
        <%= err.message %>!
    </h4>
    <% if(process.env.NODE_ENV !== "production"){ %>
        <p class="mb-0">
            <%= err.stack %>
        </p>
    <% }%>
</div>